
#control_system.py
from control_system import *


A=systf([1,2],[1,3,5,1])
print A
clpoles=rlocus(A,0.1,100,10)


sorted=zeros_like(clpoles)



sorted[0]=clpoles[0]
clpoles.pop(0)
Q=inf

for n,row in enumerate(clpoles):
	if n==0:
		test=clpoles[0]
		sorted[0]=test
	else:
		for n2 in arange(n,len(clpoles)):
			print n2
			minn=abs(row-test)
			print minn
			if Q > minn: 
				Q=minn
				Nmin=n
print "-------aaaaaa-----"
print Q
print Nmin

print "-----------------"

