from robotlib import *

x1=-0.63
X=-0.63
Y=0.69
L1=0.7
L2=0.5

R=sqrt(X**2+Y**2)
C2=(R**2-(L1**2+L2**2))/(2*L1*L2)
S2=sqrt(1-C2**2)

phi=atan2(Y,X)


# Angulo da junta 2 solucao 1
q21=atan2(S2,C2)	
alpha=atan2(L2*S2,L1+L2*C2)

q11=phi-alpha

q1=q11
q2=q21
X1=L1*sind(q1)+L2*sind(q1+q2)
Y1=L1*sind(q1)+L2*sind(q2+q2)
 
#  Angulo da junta 2 solucao 2
q22=atan2(-S2,C2)
alpha=atan2(L2*(-S2),L1+L2*C2)

q12=phi-alpha

q1=q12
q2=q22
X1=L1*sind(q1)+L2*sind(q1+q2)
Y1=L1*sind(q1)+L2*sind(q2+q2)



