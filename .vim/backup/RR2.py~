#!/usr/bin/env python
# -*- coding: utf-8 -*-

from robotlib import *


#  Calcula seno com argumento em graus
#
def sind(x):
	return sin(deg2rad(x))

#  Cálcula coseno com ângulo em graus
#
def cosd(x):
	return cos(deg2rad(x))
	
def tand(x):
	return tan(deg2rad(x))


import pdb


X=-0.63
Y=0.69
L1=0.7
L2=0.5

R=sqrt(X**2+Y**2)
C2=(R**2-(L1**2+L2**2))/(2*L1*L2)
S2=sqrt(1-C2**2)

phi=atan2(Y,X)


print "C2=",C2
print "S2=",S2
print "phi=",phi
print "\n\n"

# Angulo da junta 2 solucao 1
q21=atan2(S2,C2)	


alpha=atan2(L2*S2,L1+L2*C2)
print "alpha=",alpha

q11=phi-alpha

q1=q11
q2=q21
pdb.set_trace()

X1=L1*cosd(q1)+L2*cosd(q1+q2)
Y1=L1*sind(q1)+L2*sind(q2+q2)

print "q1=",q1
print "q2=",q2
print "X1=",X1
print "Y1=",Y1


#  Angulo da junta 2 solucao 2
print "\n\n"

q22=atan2(-S2,C2)
alpha=atan2(L2*(-S2),L1+L2*C2)
print "alpha=",alpha


q12=phi-alpha

q1=q12
q2=q22
pdb.set_trace()

X1=L1*cosd(q1)+L2*cosd(q1+q2)
Y1=L1*sind(q1)+L2*sind(q2+q2)

print "q1=",q1
print "q2=",q2
print "X1=",X1
print "Y1=",Y1



