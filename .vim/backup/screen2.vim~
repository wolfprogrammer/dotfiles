"":exec("!screen -S ipython -X stuff ".string(getline("."))."`echo -ne '\015'`")


" send line to screen
nmap <F5> :exec("!screen -S ipython -X stuff \"".getline(".")."\"`echo -ne '\015'`") <CR><CR><CR>

"send buffer to screen
nmap <F6> :exec("!screen -S ipython -X stuff \"".join(getline(1,'$'),"\n")."\"`echo -ne '\015'`") <CR><CR><CR>


"join(getline(1,'$'), "\n")

