

s=%s;
exec control_system_tools.sce

num=101
den= (s^2+20*s+101)
G1=num/den
G1=syslin('c',G);



G2=20*G1/(s+20);
G3=5*G1/(s+5);
G4=0.5*G1/(s+0.5)

H1=ufeedback(G1,1);
H2=ufeedback(G2,1);
H3=ufeedback(G3,1);
H4=ufeedback(G4,1);

t=0:0.01:20;
y1=csim('step',t,H1);
y2=csim('step',t,H2);
y3=csim('step',t,H3);
y4=csim('step',t,H4);

plot(t,y1,'b');
plot(t,y2,'r');
plot(t,y3,'y');
plot(t,y4,'g');
