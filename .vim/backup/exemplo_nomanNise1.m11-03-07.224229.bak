#  Exemplo de sistemas de controle do Norman Nise
#
#
#


#		Função de tranferência G(s)=(s+20)/[(s+1)*(s+7)*(s+50)]
# Representação do sistema em termos de pólos e zeros
#
num=[-20]
den=[-1 -7 -50]
G=zp(num,den,1)

printf(" \n\nRepresentação do sistema em função de transferência tf:\n");
A=sysout(G,"tf")


printf(" \n\nRepresentação do sistema em pólos e zeros zp:\n");
sysout(G,"zp")


printf(" \n\nRepresentação do sistema em estados de espaços ss:\n");
sysout(G,"ss")

printf(" \n\nRepresentação do sistema em todas formas:\n");
sysout(G,"all")

bode(G);
grid();


pause(inf);
