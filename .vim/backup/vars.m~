## vars

## Author: vendetta <caio@ghost>
## Created: 2012-07-14

function lst= vars ()

load("/tmp/context");

NN_=size(LST);

VALLIST=[];
NAMELIST={};

for  kk=1:NN_
	ww=LST{kk,1};
	xx=eval(ww);
	#NAMELIST=append(NAMELIST);
	#VALLIST=[VALLIST,xx];

	if is_scalar(xx)
		printf("%s=%f\n",ww,xx)
	end

end    


##zenity_list("Variable Value", {"var", "val"},; NAMELIST, num2str(VALLIST)})





endfunction
