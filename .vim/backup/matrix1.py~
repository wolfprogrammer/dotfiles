
# Exemplo de calculo de  matrizes

from numpy import *


#	Cria matriz
#
#         [3  4]
#	A=[2 15]
#         
A=[[3,4],[2,15]]
A=matrix(A)
print A

#	Cria vetor coluna x
#
#	  [ 4]
#       x=[ 5]
#
x=[4,5]
x=matrix(x)
x=x.transpose()
print x

#	Calcula produto de matrizes
#
C=A*x
print C

D=eye(2)
print D

K=D+A
print K

print 5*K


