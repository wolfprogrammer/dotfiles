#ifndef __delay_h__
#define __delay_h__

 
typedef unsigned char u8;
typedef unsigned int  u16; 
typedef unsigned long u32;
typedef char i8;
typedef int  i16; 
typedef long i32;
typedef char *  string;
 
void delay1s();
void delaysx(u8);
void delay500ms();

