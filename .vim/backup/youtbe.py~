#import os
import sys
from os import system as cmd

#os.path("/home/caio/tmp/")

cmd("cd /home/caio")
cmd("pwd")

LST=[]

while True:
	inpt=raw_input("Entre com a URL , -- para terminar\n>");
	
	if inpt=="--":
		break;

	LST.append(inpt)


print LST


for lst in LST:
	cmd("/home/caio/.bin/youtube-dl -t --console-title -c %s" % lst)


